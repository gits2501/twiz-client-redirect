{
  "name": "twiz-client-redirect",
  "version": "1.0.0",
  "description": "Redirect (Authorize) leg of OAuth 1.0a for twiz-client",
  "main": "src/Redirect.js",
  "scripts": {
    "instrument": "istanbul instrument src/Redirect.js --output test/redirect_instrumented.js",
    "browserify": "./node_modules/browserify/bin/cmd.js test/redirect.js -o test/redirect_bundle.js",
    "watch": "watchify test/redirect.js -o test/redirect_bundle.js --debug -v",
    "build": "npm run browserify && npm run watch",
    "test": "npm run instrument && npm run browserify && mocha-chrome test/mocha-test.html",
    "mocha-chrome": "nyc mocha-chrome test/mocha-test.html",
    "mocha": "mocha test/redirect.js",
    "mocha-test": "nyc mocha test/redirect.js --timeout=3000 ",
    "report":"nyc report"
  },
  "author": "github.com/gits2501",
  "license": "MIT",
  "dependencies": {
    "twiz-client-utils": "file:../twiz-client-utils"
  },
  "devDependencies": {
    "browserify": "^16.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "mocha-chrome": "^1.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.7.1"
  }
}
